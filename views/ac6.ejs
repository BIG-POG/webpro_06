<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>ac6</title>
</head>
<body>
  <h2>ARMORED CORE6登場ACの詳細</h2>
  <table>
    <tr><th>ID</th><th>AC名</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><a href="/ac6/<%= number %>"><%= row.ac_name %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/ac6/create">追加</a>
</body>
</html>